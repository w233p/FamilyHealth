<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.example.msi.familyhealth.View.TitleView
        android:id="@+id/set_titleView"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        app:background_color="@color/c6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:title="@string/set"
        app:title_type="@integer/title_back" />

    <com.example.msi.familyhealth.View.OneListView
        android:layout_width="match_parent"
        android:id="@+id/set_listview"
        android:layout_height="match_parent"
        android:layout_marginTop="56dp"
        app:layout_constraintTop_toBottomOf="@+id/set_titleView">

    </com.example.msi.familyhealth.View.OneListView>
	
    <TextView
        android:id="@+id/author"
        android:layout_width="0dp"
        android:layout_height="56dp"
        android:background="@color/c6"
        android:text="@string/author"
        android:textAlignment="center"
        android:textSize="28dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</android.support.constraint.ConstraintLayout>
